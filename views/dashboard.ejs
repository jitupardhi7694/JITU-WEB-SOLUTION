<%- include('./layout/partials/messages.ejs') %>
    <div class="dashboard">
        <div class="container">
            <div class="col-md-12">
                <div class="row">
                    <div class="card-title text-center mt-4 mb-4">Dashboard</div>

                    <div class="col-md-4">
                        <div class="card">
                            <div class="card-body mt-4">
                                <h4 class="card-title">
                                    Get In Touch Data
                                </h4>
                                <h4 class="text-center">
                                    <%= getIn%>
                                </h4>
                                <a href="/get-in-touch-table" class="btn btn-outline-success mt-2">View Report</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card">
                            <div class="card-body mt-4">
                                <h4 class="card-title">
                                    User Register Data
                                </h4>
                                <h4 class="text-center">
                                    <%= user%>
                                </h4>
                                <a href="/user/registerTable" class="btn btn-outline-success mt-2">View Report</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card">
                            <div class="card-body mt-4">
                                <h4 class="card-title">
                                    Create User Role
                                </h4>
                                <h4 class="text-center">
                                    <%= userRole%>
                                </h4>
                                <a href="/user_roles/register" class="btn btn-outline-success mt-2">Create</a>
                                <a href="/user_roles/view-role" class="btn btn-outline-success mt-2">View Report</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>